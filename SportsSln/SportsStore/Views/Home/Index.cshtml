@model ProductsListViewModel
@* <div>@ViewBag.PaginationInfo</div>
<div>@ViewBag.Data</div> *@
@foreach (var p in Model.Products) {
    <div>
        <h3>@p.Name</h3>
        @p.Description
        <h4>@p.Price.ToString("c")</h4>
    </div>
}
@* Pagination Block *@
<div page-model="@Model.PagingInfo" page-action="Index"></div>